

.c-textureMaskBg01
	position relative
	margin -30px 0
	padding 120px 0
	text-align center
	z-index 100

	$contentsGroup
		position relative
		z-index 100

	$canvasBgGroup
		position absolute
		top 0
		left 0
		width 100%
		height 100%
		z-index 0
	
		#textureMaskBg01
			position absolute
			top 0
			left 0

	
	&>.group
		&.is-contents
			@extend $contentsGroup
		&.is-canvasBg
			@extend $canvasBgGroup