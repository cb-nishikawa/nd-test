mixin m-linkModule01(tg)
	-
		var op = {
			url:       tg.url ? tg.url : "",
			blank:     tg.blank ? tg.blank : false,
			className: tg.className ? " " + tg.className : "",
			baseName:  tg.baseName ? tg.baseName : "m-linkModule01",
		}

	if(op.blank == true)
		a(href= op.url target="_blank" class= op.baseName + op.className)
			span
				block
	else
		a(href= op.url class= op.baseName + op.className)
			span
				block