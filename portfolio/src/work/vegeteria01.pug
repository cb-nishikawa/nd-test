
extends /assets/_include/_template/_t_portfolio_template01.pug

block vars
	-
		var pageOption = {
			KEY: "works",
			URL: '',
			RELATIVE_PATH: '../',
			TITLE: '',
			DESCRIPTION: '',
			KEYWORDS: '',
			SIDE: [false, "left"],
			HEADER: true,
			FOOTER: false,
			TOP_CONTENTS: false,
			BOTTOM_CONTENTS: false,
		}


//- ヘッダー
block header
	+c-globalHeader01
		
//- コンテンツ　
block contents
	section.c-workDetailSet01
		.componentWrapper
			.block.is-mainvisual(style="background-image: url('../assets/images/contents/vegeteria01_mainvisual_bg01.png')")
				+l-mqWrapper({tag:"div"})
					+l-contentsWidth({tag:"div"})
						.area.is-mainvisual03
							.m-workDetailSet01_mainvisual01
								.moduleWrapper
									span
										//- img(src="https://placehold.jp/1200x660.png", alt="")
										img(src= pageOption.RELATIVE_PATH + "assets/images/contents/vegeteria01_mainvisual01.png", alt="")
							

			.block.is-contents
				.area.is-title
					+l-mqWrapper({tag:"div"})
						+l-contentsWidth({tag:"div"})
							p.date
								span 2017-2021
							
							h2.title
								span ベジテリア ブランドサイト
							
							p.text 「きれいなカラダ、飲む野菜」をキーワードに健康と美しさをサポートするジュース&スープのお店「ベジテリア」のブランドサイトの更新・運用を担当しました。

							h3.subTitle
								span 担当業務
							
							p.list 更新・運用 / コーディング

							p.link
								a(href="https://www.rockfield.co.jp/brand/vege/" target="_blank")
									span ROCK FIELD CO.,LTD.-VEGETERIA-ベジテリア
			
			

				